{
  "name": "video-merger",
  "version": "1.0.0",
  "description": "视频合并工具",
  "main": "main.js",
  "build": {
    "appId": "com.plter.videomerger",
    "files": [
      "!src"
    ]
  },
  "scripts": {
    "start": "electron .",
    "build": "webpack ./src/index/renderer.js -o ./dist/index --mode production",
    "dev": "webpack ./src/index/renderer.js -o ./dist/index --mode development -w",
    "dist": "npm run build && electron-builder --dir"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.3.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3"
  }
}
